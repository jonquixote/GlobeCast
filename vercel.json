{
  "version": 2,
  "framework": "vite",
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node@18.x"
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/api/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/index.html"
    }
  ]
}
